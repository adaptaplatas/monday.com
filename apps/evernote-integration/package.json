            ".env",
            "src/"
        ]
    },
    "dependencies": {
        "body-parser": "^1.20.0",
        "cross-port-killer": "^1.4.0",
        "dotenv": "^16.0.1",
        "evernote": "^2.0.5",
        "express": "^4.18.1",
        "jsonwebtoken": "^8.5.1",
        "monday-sdk-js": "0.1.3",
        "node-cache": "^5.1.2",
        "node-fetch": "^2.6.1",
        "sequelize": "^6.20.1",
        "simple-oauth2": "^4.2.0",
        "sqlite3": "^5.0.8"
    },
    "devDependencies": {
        "@types/evernote": "^2.0.2",
        "@types/express": "^4.17.13",
        "@types/monday-sdk-js": "^0.1.5",
        "chai": "^4.1.2",
        "concurrently": "^5.2.0",
        "debug": "~2.6.9",
        "eslint": "^v6.8.0",
        "localtunnel": "^2.0.2",
        "mocha": "^5.0.4",
        "mock-require": "^3.0.1",
        "nodemon": "^2.0.16",
